{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "homepage": "<%= homepage %>",
  "bugs": {
    "url": "<%= repository %>/issues",
    "email": "<%= authorEmail %>"
  },
  "license": "<%= license %>",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "main": "./dist/electronMain.js",
  "repository": {
    "type": "git",
    "url": "<%= repository %>"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build-main": "webpack --config ./build/configs/webpack.electron.js",
    "build-watch-main": "webpack --mode development --config ./build/configs/webpack.electron.js --watch",
    "build-renderer": "webpack --config ./build/configs/webpack.config.js --mode production",
    "build": "npm-run-all build-main build-renderer",
    "start-renderer": "webpack serve --hot --mode development --config ./build/configs/webpack.config.js",
    "start-electron": "cross-env HTML_SERVER_URL=http://localhost:10113 electron ./dist/electronMain.js",
    "execute": "concurrently \"npm run start-renderer\" \"npm run build-watch-main\" \"npm run start-electron\"",
    "start": "npm-run-all build execute",
    "bundle": "electron-builder build --publish never",
    "deploy": "electron-builder build --publish always",
    "package": "npm-run-all build bundle"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@electron/remote": "^2.0.1",
    "electron-updater": "^4.6.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-transform-async-to-generator": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@wizebin/copy-node-modules": "^1.1.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "electron-notarize": "^1.1.1",
    "electron-rebuild": "^3.2.5",
    "eslint": "^8.3.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-react": "^7.27.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
